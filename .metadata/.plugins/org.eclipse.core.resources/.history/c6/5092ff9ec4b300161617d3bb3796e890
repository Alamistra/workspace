
public class FooBarBaz {
	public static void main(String[] args){
		for (int num=1; num <=50; num++){
			System.out.print(num);
			if ((num % 3) == 0){
				System.out.print("/n Foo");
			}
			if ((num % 5) == 0){
				System.out.print("/n Bar");
			}
			if ((num % 7) == 0){
				System.out.print("/n Baz");
			}
			System.out.println();
		}
	}
}
